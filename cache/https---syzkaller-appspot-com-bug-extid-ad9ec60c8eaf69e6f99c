  2f:	4c 0f bf eb          	movswq %bx,%r13
  33:	48 81 fb 0f ff 07 00 	cmp    $0x7ff0f,%rbx
  3a:	74 00                	je     0x3c
  3c:	48 c1 e3 20          	shl    $0x20,%rbx
</pre></div><br>
	

	

